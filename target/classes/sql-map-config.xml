<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
 	<!-- Alias 설정 -->
    <typeAliases>
    	<typeAlias type="com.inventory.app.domain.ItemVO" alias="item"/>
    	<typeAlias type="com.inventory.app.domain.ShopVO" alias="shop"/>
    	<typeAlias type="com.inventory.app.domain.UserVO" alias="user"/>
    </typeAliases>
 
 	<!-- Sql Mapper 설정 -->
    <mappers>
        <mapper resource="mybatis/mapper/ItemMapper.xml"/>
        <mapper resource="mybatis/mapper/ShopMapper.xml"/>
        <mapper resource="mybatis/mapper/UsertbMapper.xml"/>
    </mappers>
</configuration>